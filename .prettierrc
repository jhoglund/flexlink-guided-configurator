{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "singleLine": true,
        "newlineBetweenRules": false,
        "newlineBetweenSelectors": false
      }
    }
  ]
}
